<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                {{#each products}}
                    <tr>
                        <td><img src="{{this.product.img}}" alt="{{this.product.nombre}}" width="48"></td>
                        <td class="align-middle">{{this.product.nombre}}</td>
                        <td class="align-middle">${{this.product.precio}}</td>
                        <td class="afdslign-middle text-end"><button class="btn btn-dark" onclick="eliminarProducto('{{product.product._id}}')">Eliminar</button></td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>
</div>

<script src="./js/Carts.js"></script>